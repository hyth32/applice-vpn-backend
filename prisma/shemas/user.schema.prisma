model User {
  id            Int            @id @default(autoincrement())
  name          String?
  telegramId    String?
  username      String?
  freeKeyUsed   Boolean        @default(false)
  createdAt     DateTime       @default(now())
  updatedAt     DateTime       @updatedAt
  notifications Notification[]
  orders        Order[]
  keys          Key[]
}
